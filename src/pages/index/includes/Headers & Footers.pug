include mixins/_header
include mixins/_footer

section.section
    .container-fluid
        .container: .row
            -
                var headerStandardState = {
                    imgName: 'Logo',
                    links: [
                        'Home',
                        'About Us',
                        'Services',
                        'Careers',
                        'News',
                        'Documentation'
                    ],
                    btn: [
                        {
                            context: 'login',
                            border: 'border',
                            state: 'active',
                        },
                        {
                            context: 'register',
                            border: '',
                            state: 'active',
                        },
                    ]
                }

            +HeaderStandard(
                headerStandardState.imgName,
                headerStandardState.links,
                headerStandardState.btn
            )